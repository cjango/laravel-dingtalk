{
    "name": "cjango/laravel-dingtalk",
    "description": "钉钉 SDK for Laravel",
    "keywords": ["dingtalk", "laravel", "sdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "cjango",
            "email": "chenjxlg@163.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "adbario/php-dot-notation": "^2.0",
        "guzzlehttp/guzzle": "^6.2",
        "pimple/pimple": "^3.0",
        "symfony/cache": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "EasyDingTalk\\": "mingyoung/dingtalk/src/",
            "cjango\\Dingtalk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "cjango\\Dingtalk\\DingtalkProvider"
            ],
            "aliases": {
                "Dingtalk": "cjango\\Dingtalk\\Dingtalk"
            }
        }
    }
}
